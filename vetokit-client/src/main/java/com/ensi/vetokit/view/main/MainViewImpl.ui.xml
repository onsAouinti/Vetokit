<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:home="urn:import:com.ensi.vetokit.view.home"
             xmlns:sidebar="urn:import:com.ensi.vetokit.view.sidebar"
             xmlns:labs="urn:import:com.ensi.vetokit.view.labs"
             xmlns:client="urn:import:com.ensi.vetokit.view.client">

    <ui:with field="msg" type="com.ensi.vetokit.resources.AppMessages" />
    <ui:with field="res" type="com.ensi.vetokit.resources.AppResources" />

    <ui:style>
        .floatRight {
            float: right;
        }
        .sidebarShowBtn{
            color:#939393;
            /*height:42px;
            position:absolute;
            right:0;
            top:0;
            z-index:4;
            width:30px;*/
            -webkit-transition-property:width,color;
            transition-property:width,color;
            -webkit-transition-duration:100ms;
            transition-duration:100ms;
            -webkit-transition-timing-function:ease-in;
            transition-timing-function:ease-in
        }

    </ui:style>
    <g:HTMLPanel ui:field="test" >
        <b:TabPanel width="100%" height="100%">
            <b:TabPane heading="Agenda">
                <home:HomeViewImpl/>
            </b:TabPane>
            <b:TabPane heading="Client">
                <client:ClientViewImpl/>
            </b:TabPane>
            <b:TabPane heading="Laboratory">
                <labs:LabViewImpl/>
            </b:TabPane>
        </b:TabPanel>
    </g:HTMLPanel>
</ui:UiBinder>