<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             xmlns:formitem="urn:import:com.ensi.vetokit.view.socle.formitem">

    <ui:with field="msg" type="com.ensi.vetokit.resources.AppMessages" />
    <ui:style src="../Lab.css" />

    <g:HTMLPanel>
        <b:Modal ui:field="modal" title="{msg.labModal}" backdrop="STATIC" keyboard="true" animation="true">

            <formitem:TextFormItem ui:field="raisonSociale" maxLength="50" label="{msg.raisonSociale}"
                                   labelWidth="150px" enabled="true"/>
            <formitem:TextFormItem ui:field="email" maxLength="50" label="{msg.email}"
                                   labelWidth="150px" enabled="true"/>
            <b:ModalFooter>
                <b:Button ui:field="saveButton" icon="OK_SIGN" text="{msg.saveButton}" />
            </b:ModalFooter>
        </b:Modal>

    </g:HTMLPanel>

</ui:UiBinder>