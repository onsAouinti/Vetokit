<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:c="urn:import:com.bradrydzewski.gwt.calendar.client"
             xmlns:s="urn:import:com.ensi.vetokit.client.view.socle"
             xmlns:dp="urn:import:com.google.gwt.user.datepicker.client">
    <ui:style>

    </ui:style>
    <g:HTMLPanel>
        <g:HorizontalPanel>
            <g:DecoratorPanel ui:field="datePickerDecorator">
                <dp:DatePicker ui:field="datePicker"/>
            </g:DecoratorPanel>
            <g:VerticalPanel>
                <g:DecoratedTabBar ui:field="calendarViewsTabBar"/>
                <g:DecoratorPanel ui:field="dayViewDecorator">
                    <c:Calendar ui:field="calendar"/>
                </g:DecoratorPanel>
            </g:VerticalPanel>
        </g:HorizontalPanel>
    </g:HTMLPanel>
</ui:UiBinder>